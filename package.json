{
    "name": "vite-template-vue-ts-electron",
    "version": "1.0.0",
    "license": "MIT",
    "main": "main.js",
    "scripts": {
        "dev": "vite --debug",
        "serve": "vite preview",
        "build": "vite build",
        "build:scripts": "tsc --build ./tsconfig.scripts.json",
        "clean": "del-cli \"dist\" \"build\" \"main.js\" \"preload.js\" \"main.js.map\" \"preload.js.map\" \"scripts/*.js\" \"scripts/*.map\"",
        "clean:dist": "del-cli \"dist\"",
        "clean:build": "del-cli \"build\"",
        "clean:scripts": "del-cli \"scripts/*.js\" \"scripts/*.map\"",
        "clean:dev": "del-cli \"main.js\" \"preload.js\" \"main.js.map\" \"preload.js.map\"",
        "dist:all": "electron-builder -c ./electron-builder.yml",
        "dist:win": "electron-builder -w -c ./electron-builder.yml",
        "dist:mac": "electron-builder -m -c ./electron-builder.yml",
        "dist:linux": "electron-builder -l -c ./electron-builder.yml",
        "dist:win:x64": "electron-builder -w --x64 -c ./electron-builder.yml",
        "dist:mac:x64": "electron-builder -m --x64 -c ./electron-builder.yml",
        "dist:linux:x64": "electron-builder -l --x64 -c ./electron-builder.yml",
        "dist:win:ia32": "electron-builder -w --ia32 -c ./electron-builder.yml",
        "dist:mac:ia32": "electron-builder -m --ia32 -c ./electron-builder.yml",
        "dist:linux:ia32": "electron-builder -l --ia32 -c ./electron-builder.yml",
        "dist:win:armv7l": "electron-builder -w --armv7l -c ./electron-builder.yml",
        "dist:mac:armv7l": "electron-builder -m --armv7l -c ./electron-builder.yml",
        "dist:linux:armv7l": "electron-builder -l --armv7l -c ./electron-builder.yml",
        "dist:win:arm64": "electron-builder -w --arm64 -c ./electron-builder.yml",
        "dist:mac:arm64": "electron-builder -m --arm64 -c ./electron-builder.yml",
        "dist:linux:arm64": "electron-builder -l --arm64 -c ./electron-builder.yml",
        "postinstall": "electron-builder install-app-deps"
    },
    "dependencies": {
        "vue": "^3.0.5",
        "vue-router": "^4.0.5",
        "vuex": "^4.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.13.1",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.0",
        "@babel/plugin-proposal-numeric-separator": "^7.12.13",
        "@babel/plugin-proposal-optional-chaining": "^7.13.0",
        "@babel/plugin-proposal-throw-expressions": "^7.12.13",
        "@babel/plugin-syntax-import-meta": "^7.10.4",
        "@babel/plugin-syntax-top-level-await": "^7.12.13",
        "@babel/plugin-transform-runtime": "^7.13.2",
        "@babel/preset-env": "^7.13.0",
        "@babel/runtime": "^7.13.2",
        "@mapbox/node-pre-gyp": "^1.0.1",
        "@rollup/plugin-alias": "^3.1.1",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@rollup/pluginutils": "^4.1.0",
        "@types/babel__core": "^7.1.12",
        "@types/cross-spawn": "^6.0.2",
        "@types/fs-extra": "^9.0.7",
        "@types/node": "^14.14.31",
        "@vitejs/plugin-vue": "^1.1.4",
        "@vitejs/plugin-vue-jsx": "^1.1.0",
        "@vue/compiler-sfc": "^3.0.5",
        "chalk": "^4.1.0",
        "chokidar": "^3.5.1",
        "del-cli": "^3.0.1",
        "electron": "^12.0.2",
        "electron-builder": "^22.10.5",
        "electronmon": "^1.1.2",
        "esbuild": "^0.10.2",
        "fs-extra": "^9.1.0",
        "node-abi": "^2.21.0",
        "rollup": "^2.39.1",
        "rollup-plugin-esbuild": "^3.0.2",
        "rollup-plugin-terser": "^7.0.2",
        "tslib": "^2.1.0",
        "typescript": "^4.1.3",
        "vite": "^2.1.3"
    }
}